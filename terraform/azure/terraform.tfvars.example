resource_group_name = "secure-training-rg"
location            = "eastus"
storage_account_name = "securetrainingstorage"
key_vault_name      = "secure-training-kv"
log_analytics_workspace_name = "secure-training-workspace"
vm_admin_username   = "azureuser"
