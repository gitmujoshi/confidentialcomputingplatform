allowed_ip_ranges   = ["10.0.0.0/16", "192.168.1.0/24"]

tags = {
  Environment = "Production"
  Project     = "SecureTraining"
  ManagedBy   = "Terraform"
  Owner       = "YourName"
  CostCenter  = "ML-Training"
} 